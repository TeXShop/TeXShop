% Example found somewhere on the web

u#:=4/9pt#; 
pixels_per_inch := 10;
%cull;

define_pixels(u); 

beginchar(666,13u#,16u#,5u#);"Letter beta"; 

x1=2u; x2=x3=3u; 

bot y1=-5u; y2=8u; y3=14u; 

x4=6.5u; top y4=h; 

z5=(10u,12u); 

z6=(7.5u,7.5u); z8=z6; 

z7=(4u,7.5u); 

z9=(11.5u,2u); 

z0=(5u,u); 

penpos1(2u,20); 

penpos2(.5u,0); 

penpos3(u,-45); 

penpos4(.8u,-90); 

penpos5(1.5u,-180); 

penpos6(.4u,150); 

penpos7(.4u,0); 

penpos8(.4u,210); 

penpos9(1.5u,-180); 

penpos0(.3u,20); 

pickup pencircle; 

penstroke z1e..z2e..z3e..z4e..z5e..z6e..{up}z7e..z8e..z9e..{up}z0e; 

labels(range 1 thru 9); 

endchar; 
end.
